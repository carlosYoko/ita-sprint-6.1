{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run remove-tests-folder",
    "remove-tests-folder": "rm -rf dist/__tests__",
    "build:watch": "tsc --build tsconfig.json --watch",
    "test": "jest --runInBand --verbose",
    "allTests": "npm run test:players && npm run test:games",
    "test:games": "npx jest apisql_games.test.ts",
    "test:players": "npx jest apisql_players.test.ts",
    "test:ranking": "npx jest apisql_ranking.test.ts",
    "docker": "docker-compose up",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "start:sql": "node dist/mysql/app.js",
    "start:mongodb": "node dist/mongodb/app.js",
    "dev:sql": "nodemon --exec ts-node src/mysql/server.ts",
    "dev:mongodb": "nodemon --exec ts-node src/mongodb/app.ts"
  },
  "keywords": [],
  "authors": [
    "Gabriel",
    "Solida",
    "Carlos Gimenez <carlosgibe@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@types/supertest": "^6.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.0",
    "supertest": "^6.3.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^5.8.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
