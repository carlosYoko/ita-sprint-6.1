{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "sourceMap": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "noEmitOnError": false,
    "removeComments": true,
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "*": ["./src/*"]
    }
  },
  "include": [
    "./src/**/*",
    "./generated/**/*",
    "src/mysql/.endpoints copy.ts",
    "src/mysql/.endpoints.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".src/__tests__",
    "prisma/generated",
    "prisma/migrations"
  ]
}
